{
  "extends": [
    "html-validate:recommended"
  ],
  "rules": {
    "wcag/h30": 0,
    "no-style-tag": 2,
    "no-unknown-elements": 2,
    "prefer-button": 1
  },
  "elements": [
    "html5",
    {
      "form": {
        "attributes": {
          "name": {
            "required": true
          }
        }
      }
    }
  ]
}
